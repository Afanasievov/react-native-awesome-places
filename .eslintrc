
{
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "plugins": ["react", "jsx-a11y", "import", "prettier"],
  "rules": {
    "prettier/prettier": "error",
    "react/jsx-filename-extension": ["off"],
    "linebreak-style": ["off"],
    "no-undef": ["error"],
    "react/sort-comp": ["off"],
    "react/prefer-stateless-function": ["off"],
    "react/prop-types": 0,
    "arrow-parens": "off",
    "no-mixed-operators": "warn"
  },
  "globals": {
    "it": false,
    "expect": false,
    "describe": false,
    "navigator": false
  }
}